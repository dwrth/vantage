{
  "name": "@vantage/page-builder",
  "version": "0.0.3",
  "description": "A framework for building website builders",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "example:dev": "cd example && npm run dev",
    "example:build": "cd example && npm run build",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwrth/vantage.git"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-rnd": "git+https://git@github.com/dwrth/react-rnd.git"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "prettier": "^3.8.1",
    "react-rnd": "git+https://git@github.com/dwrth/react-rnd.git",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "page-builder",
    "drag-and-drop",
    "responsive",
    "react",
    "editor"
  ],
  "author": "David Werth <@dwrth>",
  "license": "MIT"
}
